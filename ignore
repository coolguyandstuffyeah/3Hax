			},

			{

				Type = "Split",

			},

			{

				Name = "Kill Tool",
				Type = "Button",
				Desc = "Click the person you want to kill, Equip a sword and slash.",
				Action = function()

					local Kill = Instance.new("Tool",Plr.Backpack)

					Kill.Name = "Kill"
					Kill.ToolTip = "Click the person you want to kill, Equip a sword and slash."
					Kill.RequiresHandle = false
					Kill.CanBeDropped = false

					local Handle = Instance.new("Part", Kill)

					Handle.CanCollide = false
					Handle.Name = "Handle"
					Handle.Size = Vector3.new(0.001, 0.001, 0.001)
					Handle.Transparency = 1

					local Do = 0

					Kill.Activated:Connect(function()

						if Mouse.Target.Parent:FindFirstChild("HumanoidRootPart") or Mouse.Target.Parent.Parent:FindFirstChild("HumanoidRootPart") then

							local Head = Mouse.Target.Parent.Head or Mouse.Target.Parent.Parent.Head
							local Character = Head.Parent

							if not Character:FindFirstChild("DeathLine") then

								local Outline = Instance.new("Highlight",Character)

								Outline.OutlineColor = Color3.new(1,0,0)
								Outline.OutlineTransparency = 0
								Outline.FillTransparency = 1
								Outline.Name = "DeathLine"

								game.TweenService:Create(Outline,TweenInfo.new(0.3),{OutlineTransparency = 0}):Play()

								local Tool = Plr.Character:FindFirstChildOfClass("Tool")

								local Handle = Tool:FindFirstChild("Handle")

								while Plr.Character and Character do

									local Human = Character:FindFirstChildWhichIsA("Humanoid")

									if not Human or Human.Health <= 0 then
										break
									end

									Tool = Plr.Character:FindFirstChildOfClass("Tool")

									if not Tool then

										break

									end

									Handle = Tool:FindFirstChild("Handle")

									if not Handle then

										break

									end

									Character["Left Leg"]:BreakJoints()
									Character["Left Leg"].Velocity = Vector3.new(0,0,0)
									Character["Left Leg"].Position = Handle.Position + Vector3.new(math.random(-200,200) / 150,math.random(-200,200) / 150,math.random(-200,200) / 150)

									task.wait()

								end

								game.TweenService:Create(Outline,TweenInfo.new(0.3),{OutlineTransparency = 1}):Play()

								wait(0.3)

								Outline:Destroy()

							end

						end

					end)

				end,

			},

