						if Thing:FindFirstChild("Owner") and Thing:FindFirstChild("Owner"):IsA("StringValue") and Thing:FindFirstChild("Owner").Value == Plr.Name then

							Base = Thing

						end

					end

					Look.Size = Vector3.new(0,0,0)
					Look.CanCollide = false
					Look.Transparency = 1
					Look.Anchored = true
					Look.Name = "Freecam"
					Look.CFrame = game.Workspace.CurrentCamera.CFrame

					game.Workspace.CurrentCamera.CameraSubject = Look

					Plr.CameraMinZoomDistance = 1
					Plr.CameraMaxZoomDistance = 1

					local Forward = 0
					local Right = 0
					local Up = 0

					local ScrollFunc = UIS.InputChanged:Connect(function(Input)

						if Input.UserInputType == Enum.UserInputType.MouseWheel then
							if Input.Position.Z > 0 then
								Forward += 1
							elseif Input.Position.Z < 0 then
								Forward -= 1
							end
						end

					end)

					local Destroyed = false

					spawn(function()

						while not Destroyed do

							task.wait(0.1)

							for i=1,10 do

								game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
								game:GetService("ReplicatedStorage").SpawnSuperBlock:FireServer()
								game:GetService("ReplicatedStorage").SpawnGalaxyBlock:FireServer()
								game:GetService("ReplicatedStorage").SpawnDiamondBlock:FireServer()
								game:GetService("ReplicatedStorage").SpawnRainbowBlock:FireServer()

							end

						end

					end)

					local Run = game["Run Service"].RenderStepped:Connect(function()

						task.wait()

						if UIS:IsKeyDown(Enum.KeyCode.W) then

							Forward += 0.05

						end

						if UIS:IsKeyDown(Enum.KeyCode.S) then

							Forward -= 0.05

						end

						if UIS:IsKeyDown(Enum.KeyCode.D) then

							Right += 0.05

						end

						if UIS:IsKeyDown(Enum.KeyCode.A) then

							Right -= 0.05

						end

						if UIS:IsKeyDown(Enum.KeyCode.Q) then

							Up -= 0.05

						end

						if UIS:IsKeyDown(Enum.KeyCode.E) then

							Up += 0.05

						end

						game.Workspace.CurrentCamera.CameraSubject = Look

						if not FreeMove then

							Plr.Character:MoveTo(Base.SpawnLocation.Position + Vector3.new(0,2,0))

						end

						Look.CFrame = CFrame.new(Look.Position + (Look.CFrame.LookVector * Forward) + (Look.CFrame.RightVector * Right) + (Look.CFrame.UpVector * Up)) * game.Workspace.CurrentCamera.CFrame.Rotation

						Forward *= 0.9
						Right *= 0.9
						Up *= 0.9

					end)

