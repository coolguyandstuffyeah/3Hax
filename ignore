		{

			Name = "Lucky Blocks Battlegrounds",
			Type = "Button",
			Desc = "This is the most fun game to hack. Gives you insane powers, and you can even control the entire game with a command that took ages to make.",
			Action = function()

				local TeleportService = game:GetService("TeleportService")
				local Id = 662417684

				TeleportService:Teleport(Id, Plr)

			end,

		},
		
		{

			Name = "Lucky Blocks Battlegrounds",
			Type = "Button",
			Desc = "This is the most fun game to hack. Gives you insane powers, and you can even control the entire game with a command that took ages to make.",
			Action = function()

				local TeleportService = game:GetService("TeleportService")
				local Id = 662417684

				TeleportService:Teleport(Id, Plr)

			end,

		},

	}

end

-- Setup

LoadCommands()

