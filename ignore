-- Check

if game.CoreGui:FindFirstChild("3Hax") then

	game.TweenService:Create(game.CoreGui:FindFirstChild("3Hax").Title,TweenInfo.new(0.5),{Size = UDim2.new(0.077, 0,0, 0)}):Play()
	wait(0.5)
	game.CoreGui:FindFirstChild("3Hax"):Destroy()

end

